{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "semi": "error",
        "quotes": [2, "single", { "avoidEscape": true }],
        "no-unreachable":"error",
        "no-unused-vars":"error",
        "no-unused-labels":"error"
    }
}